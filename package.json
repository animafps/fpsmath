{
  "name": "fpsmath",
  "version": "1.9.0",
  "main": "index.js",
  "scripts": {
    "test": "npm run lint",
    "lint": "eslint .",
    "start": "node dist/index.js",
    "start-dev": "tsc && node dist/index.js",
    "lint:fix": "eslint --fix .",
    "cz": "cz",
    "prettier": "prettier --write .",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AnimaFPS/FPSMath.git"
  },
  "keywords": [],
  "author": "AnimaFPS",
  "license": "MIT",
  "description": "A Discord bot designed to convert sensitivities, fovs, mouse feel across themselves and preset games",
  "dependencies": {
    "better-sqlite3": "^7.1.2",
    "common-tags": "^1.8.0",
    "discord.js": "^12.5.1",
    "discord.js-commando": "^0.12.2",
    "dotenv": "^8.2.0",
    "mathjs": "^9.0.0",
    "node-fetch": "^2.6.1",
    "topgg-autoposter": "^1.1.6",
    "winston": "^3.3.3"
  },
  "engines": {
    "node": "^14.15.5"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@types/common-tags": "^1.8.0",
    "@types/mathjs": "^6.0.11",
    "@types/node": "^14.14.20",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.16.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-security": "^1.4.0",
    "prettier": "2.2.1",
    "semantic-release": "^17.3.7",
    "typescript": "^4.1.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "docs/CHANGELOG.md"
        }
      ],
      "@semantic-release/npm",
      [
        "@semantic-release/github",
        {
          "assets": [
            "docs/CHANGELOG.md",
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
